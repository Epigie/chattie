{
  "expo": {
    "name": "chattie",
    "slug": "chattie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chattie.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/chattiesplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-chattie.png",
        "backgroundColor": "#ffffff"
      },
      "package": "mkul.chattie",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d4329554-342c-4a65-980b-6b15d5ad88fc"
      }
    },
    "owner": "mkul11"
  }
}
